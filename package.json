{
  "name": "svui",
  "version": "0.0.1",
  "description": "A Svelte component library loosely based on Material Design",
  "keywords": [
    "svelte",
    "sveltejs",
    "component",
    "component-library",
    "material"
  ],
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish": "npm publish ./package --access public",
    "test": "ts-mocha -p tsconfig.test.json --paths tests/**/*.ts -r dotenv/config"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.4.19",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-loader": "^8.2.3",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-svelte3": "^3.2.1",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.49.7",
    "svelte": "^3.46.4",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.3",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0",
    "tslib": "^2.3.1",
    "typescript": "~4.5.4"
  },
  "type": "module"
}
